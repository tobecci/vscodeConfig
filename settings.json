{
	////////////////////////////////////////////////////////////////////////////////////////////////////
	//languages
	"intelephense.files.exclude": [
		"**/.git/**",
		"**/.svn/**",
		"**/.hg/**",
		"**/CVS/**",
		"**/.DS_Store/**",
		"**/node_modules/**",
		"**/bower_components/**",
		"**/vendor/**/{Tests,tests}/**",
		"**/.history/**"
	],
	"[vue]": {
		"editor.defaultFormatter": "Vue.volar"
	},
	"files.associations": {
		"*.module": "php",
	},
	"[php]": {
		"editor.defaultFormatter": "DEVSENSE.phptools-vscode"
	},
	//   "editor.defaultFormatter": "esbenp.prettier-vscode",
	"[javascript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"[blade]": {
		"editor.defaultFormatter": "shufo.vscode-blade-formatter"
	},
	"[makefile]": {},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		// "**/node_modules": true,
		"**/.idea": true,
	},
	"[dart]": {
		"editor.formatOnSave": true,
		"editor.formatOnType": true,
		"editor.rulers": [
			80
		],
		"editor.selectionHighlight": false,
		"editor.suggestSelection": "first",
		"editor.tabCompletion": "onlySnippets",
		"editor.wordBasedSuggestions": "off"
	},
	"dart.flutterSdkPath": "/opt/flutter/bin",
	"[nginx]": {
		"editor.defaultFormatter": "raynigon.nginx-formatter"
	},
	"[typescript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"http.experimental.systemCertificatesV2": true,
	"[yaml]": {
		"editor.defaultFormatter": ""
	},
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[css]": {
		"editor.defaultFormatter": "vscode.css-language-features"
	},
	"typescript.updateImportsOnFileMove.enabled": "always",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"php.format.rules.alignConsecutiveAssignments": true,
	"php.format.rules.arrayInitializersNewLineAfterLastElement": true,
	"php.format.rules.arrayInitializersAlignKeyValuePairs": true,
	"php.format.rules.callParametersNewLineAfterLeftParen": false,
	////////////////////////////////////////////////////////////////////////////////////////////////////
	//security
	"security.workspace.trust.untrustedFiles": "open",
	////////////////////////////////////////////////////////////////////////////////////////////////////
	//explorer
	"explorer.decorations.badges": true,
	////////////////////////////////////////////////////////////////////////////////////////////////////
	//editor
	"editor.fontFamily": "SF Mono",
	"editor.fontSize": 13,
	"editor.lineHeight": 2.5,
	"window.zoomLevel": 1.7,
	"workbench.startupEditor": "none",
	"editor.comments.ignoreEmptyLines": true,
	"workbench.editor.mouseBackForwardToNavigate": false,
	"workbench.editor.restoreViewState": true,
	"workbench.editor.sharedViewState": true,
	"workbench.reduceMotion": "on",
	"editor.guides.indentation": true,
	"window.density.editorTabHeight": "compact",
	"workbench.editor.pinnedTabsOnSeparateRow": true,
	"editor.renderWhitespace": "all",
	"editor.renderFinalNewline": "on",
	"window.closeWhenEmpty": false,
	"editor.wrappingStrategy": "simple",
	"window.openFilesInNewWindow": "off",
	"window.openFoldersInNewWindow": "default",
	"editor.selectionHighlight": false,
	// "editor.tabCompletion": "off",
	"editor.scrollbar.verticalScrollbarSize": 30,
	"editor.wordWrap": "on",
	"editor.formatOnSave": false,
	"editor.insertSpaces": false,
	"editor.guides.bracketPairsHorizontal": true,
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"editor.guides.bracketPairs": true,
	"editor.matchBrackets": "always",
	"gitlens.blame.highlight.locations": [
		"gutter",
	],
	"editor.renderLineHighlight": "none",
	"editor.guides.highlightActiveBracketPair": true,
	"editor.guides.highlightActiveIndentation": "always",
	"editor.occurrencesHighlight": "off",
	////////////////////////////////////////////////////////////////////////////////////////////////////
	//cursor
	"editor.cursorBlinking": "solid",
	"editor.cursorWidth": 3,
	"editor.cursorStyle": "block-outline",
	"editor.wrappingIndent": "indent",
	"workbench.editor.pinnedTabSizing": "normal",
	"editor.cursorSmoothCaretAnimation": "off",
	////////////////////////////////////////////////////////////////////////////////////////////////////
	//misc
	"editor.autoClosingBrackets": "always",
	"editor.renderControlCharacters": true,
	"editor.autoClosingDelete": "always",
	"editor.autoClosingOvertype": "never",
	"editor.comments.insertSpace": true,
	"workbench.tree.renderIndentGuides": "always",
	"workbench.tree.indent": 40,
	"editor.unicodeHighlight.invisibleCharacters": true,
	"editor.foldingImportsByDefault": true,
	"editor.formatOnType": true,
	"editor.showFoldingControls": "always",
	"window.newWindowDimensions": "maximized",
	"explorer.compactFolders": false,
	"window.restoreWindows": "none",
	"editor.autoClosingComments": "always",
	"editor.autoIndent": "advanced",
	"editor.colorDecorators": true,
	"editor.copyWithSyntaxHighlighting": false,
	////////////////////////////////////////////////////////////////////////////////////////////////////
	//extensions
	"extensions.ignoreRecommendations": true,
	////////////////////////////////////////////////////////////////////////////////////////////////////
	// find
	"editor.find.loop": false,
	////////////////////////////////////////////////////////////////////////////////////////////////////
	// file name colors
	"workbench.editor.decorations.colors": false,
	"workbench.activityBar.location": "top",
	"editor.detectIndentation": false,
	"editor.tabSize": 6,
	"workbench.editor.navigationScope": "editor",
	////////////////////////////////////////////////////////////////////////////////////////////////////
	//scroll limit
	"editor.scrollBeyondLastColumn": 0,
	"editor.scrollBeyondLastLine": false,
	////////////////////////////////////////////////////////////////////////////////////////////////////
	//preview
	"workbench.editor.enablePreview": false,
	"workbench.editor.enablePreviewFromQuickOpen": false,
	"workbench.editor.enablePreviewFromCodeNavigation": false,
	"editor.suggest.preview": true,
	"editor.autoSurround": "languageDefined",
	////////////////////////////////////////////////////////////////////////////////////////////////////
	// where to open files
	"workbench.editor.revealIfOpen": false,
	////////////////////////////////////////////////////////////////////////////////////////////////////
	// pinned
	"workbench.editor.preventPinnedEditorClose": "keyboardAndMouse",
	////////////////////////////////////////////////////////////////////////////////////////////////////
	//groups
	"workbench.editor.closeEmptyGroups": false,
	////////////////////////////////////////////////////////////////////////////////////////////////////
	// minimap
	"editor.minimap.enabled": false,
	"editor.minimap.renderCharacters": false,
	"editor.minimap.scale": 2,
	"editor.minimap.size": "proportional",
	"editor.minimap.maxColumn": 50,
	"editor.minimap.showSlider": "always",
	"editor.minimap.autohide": false,
	////////////////////////////////////////////////////////////////////////////////////////////////////
	//tabbar
	"window.commandCenter": false,
	"window.menuBarVisibility": "hidden",
	"workbench.editor.showTabs": "single",
	"workbench.editor.limit.enabled": true,
	"workbench.editor.limit.value": 3,
	"workbench.editor.limit.perEditorGroup": false,
	"workbench.editor.limit.excludeDirty": true,
	"editor.tabFocusMode": true,
	"workbench.editor.tabActionCloseVisibility": false,
	"workbench.editor.tabActionUnpinVisibility": true,
	////////////////////////////////////////////////////////////////////////////////////////////////////
	//suggestions
	"editor.wordBasedSuggestions": "off",
	"editor.suggest.matchOnWordStartOnly": false,
	"editor.suggestSelection": "first",
	"editor.suggest.showWords": false,
	"editor.suggest.showInlineDetails": true,
	"editor.inlineSuggest.enabled": true,
	"editor.suggest.showColors": true,
	"editor.suggest.showCustomcolors": true,
	"editor.suggest.showValues": true,
	"editor.suggest.showVariables": true,
	"editor.suggest.showFields": true,
	"editor.suggest.showEnums": true,
	"editor.suggest.showProperties": true,
	"editor.suggest.showEnumMembers": true,
	"editor.suggest.showTypeParameters": true,
	"editor.suggestOnTriggerCharacters": false,
	"editor.quickSuggestionsDelay": 1000,
	"editor.quickSuggestions": {
		"other": "off",
		"comments": "off",
		"strings": "off",
	},
	////////////////////////////////////////////////////////////////////////////////////////////////////
	//quick open
	"search.quickOpen.includeHistory": true,
	"search.quickOpen.includeSymbols": true,
	"workbench.quickOpen.closeOnFocusLost": true,
	"workbench.quickOpen.preserveInput": false,
	////////////////////////////////////////////////////////////////////////////////////////////////////
	//terminal
	"terminal.integrated.fontSize": 13,
	"terminal.integrated.lineHeight": 1.3,
	"terminal.integrated.hideOnStartup": "always",
	"terminal.external.linuxExec": "alacritty",
	"terminal.integrated.accessibleViewPreserveCursorPosition": true,
	"terminal.integrated.confirmOnExit": "hasChildProcesses",
	"terminal.integrated.drawBoldTextInBrightColors": true,
	"terminal.integrated.copyOnSelection": true,
	"terminal.integrated.cursorBlinking": false,
	"terminal.integrated.cursorStyle": "underline",
	"terminal.integrated.cursorStyleInactive": "underline",
	"terminal.integrated.defaultProfile.linux": "bash",
	"terminal.integrated.tabs.location": "right",
	"terminal.integrated.commandsToSkipShell": [
		"workbench.action.openRecent",
		"workbench.action.closePanel",
		"workbench.action.closeSidebar",
		"workbench.action.focusActiveEditorGroup",
		"workbench.action.toggleEditorWidths",
		"workbench.action.focusSideBar",
		"workbench.action.terminal.focus",
		"workbench.action.openSettings",
		"workbench.action.openSettingsJson",
		"workbench.action.openGlobalKeybindingsFile",
		"workbench.action.navigateBack",
		"workbench.action.navigateForward",
		"workbench.action.evenEditorWidths",
		"workbench.action.splitEditorRight",
		"workbench.action.quickOpen",
		"workbench.action.maximizeEditorHideSidebar",
		"workbench.action.closeAuxiliaryBar",
		"closeFindWidget",
		"workbench.action.gotoSymbol",
		"type",
		"workbench.action.closeEditorsInOtherGroups",
		"workbench.action.closeOtherEditors",
		"workbench.action.unpinEditor",
		"workbench.action.pinEditor",
		"workbench.action.togglePanel",
		"revealInExplorer",
		"editor.action.joinLines",
		"editor.action.formatSelection",
		"workbench.action.files.save",
		"editor.action.formatChanges"
	],
	"terminal.integrated.shellIntegration.history": 5,
	"terminal.integrated.enablePersistentSessions": false,
	////////////////////////////////////////////////////////////////////////////////////////////////////
	//panel
	"workbench.panel.opensMaximized": "always",
	"workbench.panel.defaultLocation": "bottom",
	////////////////////////////////////////////////////////////////////////////////////////////////////
	//delays
	"editor.hover.delay": 1000,
	////////////////////////////////////////////////////////////////////////////////////////////////////
	//folding
	////////////////////////////////////////////////////////////////////////////////////////////////////
	"editor.foldingHighlight": true,
	////////////////////////////////////////////////////////////////////////////////////////////////////
	//telemetry
	"telemetry.telemetryLevel": "off",
	"gitlens.telemetry.enabled": false,
	"redhat.telemetry.enabled": false,
	"docker-compose.enableTelemetry": false,
	//
	"workbench.colorTheme": "GitHub Dark High Contrast",
	"update.mode": "manual",
	"breadcrumbs.enabled": false,
	////////////////////////////////////////////////////////////////////////////////////////////////////
}